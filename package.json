{
  "name": "acadmate-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "postinstall": "prisma generate",
    "seed": "tsx src/seed/acadmate-data.ts",
    "seed:batches": "tsx src/scripts/seedBatches.ts",
    "seed:questions": "tsx src/scripts/seedSampleQuestions.ts",
    "seed:all": "npm run seed && npm run seed:batches && npm run seed:questions",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset",
    "deploy:setup": "node scripts/deploy.js",
    "prepare:deploy": "node prepare-deployment.js",
    "migrate:deploy": "node migrate-deploy.js",
    "railway:deploy": "npm run build && npm run deploy:setup"
  },
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "prisma": "^6.16.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.11",
    "@types/node": "^24.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.6.3"
  }
}
